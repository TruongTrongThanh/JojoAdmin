{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "esModuleInterop": true,
    "importHelpers": true,
    "experimentalDecorators": true
  },
  "types": [
    "node"
  ],
  "include": [
      "./src/renderers/*"
  ],
  "exclude": [
    "node_modules"
  ]
}